<odoo>
    <data>
        <template id="external_layout_header_new">
            <div class="header">
                <table border="0">
                    <tr>
                        <th align="left">
                            <div width="30%" align="left">
                                <img align="left" src="/maintenance_reports/static/description/logo2.png"
                                     style="width:350px;height:80px;margin-left:580px;"/>
                            </div>
                        </th>

                    </tr>

                </table>


            </div>

            <div class="article o_report_layout_standard" t-att-data-oe-model="o and o._name"
                 t-att-data-oe-id="o and o.id" t-att-data-oe-lang="o and o.env.context.get('lang')">
                <t t-call="web.address_layout"/>
                <t t-raw="0"/>
            </div>


        </template>
         <template id="external_layout_header_new_visit">
            <div class="header">


            </div>

            <div class="article o_report_layout_standard" t-att-data-oe-model="o and o._name"
                 t-att-data-oe-id="o and o.id" t-att-data-oe-lang="o and o.env.context.get('lang')">
                <t t-call="web.address_layout"/>
                <t t-raw="0"/>
            </div>


        </template>


        <template id="external_layout_footer_new">
            <div class="footer" style="width:auto;margin-right:0;margin-left:-10px;padding:0;">
                <center>
                    <p style="font-size:13px;color: #2ecc71 ">For any questions, concerns, or complaints, please contact
                        Schneider Electric Customer Care Center at
                        <br/>
                        esupport.middleeast@schneider-electric.com
                    </p>
                </center>
            </div>
        </template>

        <template id="external_layout_footer_new_visit">
            <div class="footer" style="width:auto;margin-right:0;margin-left:-10px;padding:0;">
                <center>
                    <p style="font-size:15px; ">196 Sameh Gedo St, 6th district ,In Front Of mall of Arabia ,6th of October ,Giza
                    </p>
<!--                    <p style="font-size:13px; ">-->
<!--                        In Front Of mall of Arabia ,6th of October ,Giza-->
<!--                    </p>-->

                </center>
            </div>
        </template>


        <template id="external_layout_custom">
            <t t-if="not o" t-set="o" t-value="doc"/>

            <t t-if="not company">
                <!-- Multicompany -->
                <t t-if="company_id">
                    <t t-set="company" t-value="company_id"/>
                </t>
                <t t-elif="o and 'company_id' in o">
                    <t t-set="company" t-value="o.company_id.sudo()"/>
                </t>
                <t t-else="else">
                    <t t-set="company" t-value="res_company"/>
                </t>
            </t>


            <t t-call="maintenance_reports.external_layout_header_new">
                <t t-raw="0"/>
            </t>
            <t t-call="maintenance_reports.external_layout_footer_new"/>

        </template>
<template id="external_layout_custom_visit">
            <t t-if="not o" t-set="o" t-value="doc"/>

            <t t-if="not company">
                <!-- Multicompany -->
                <t t-if="company_id">
                    <t t-set="company" t-value="company_id"/>
                </t>
                <t t-elif="o and 'company_id' in o">
                    <t t-set="company" t-value="o.company_id.sudo()"/>
                </t>
                <t t-else="else">
                    <t t-set="company" t-value="res_company"/>
                </t>
            </t>


            <t t-call="maintenance_reports.external_layout_header_new_visit">
                <t t-raw="0"/>
            </t>
            <t t-call="maintenance_reports.external_layout_footer_new_visit"/>

        </template>


    </data>
</odoo>